package com.hcl.collections;

interface IHello{
	void sayHello();
}



public class Lambda2 {
	public static void main(String[] args) {
		IHello h1=() ->{
			System.out.println("hello from rishob");
		};
		
		
		IHello h2=() ->{
			System.out.println("hello from lakshmi");
		};
		
		IHello h3=() ->{
			System.out.println("hello from laksh");
		};
		
		h1.sayHello();
		h2.sayHello();
		h3.sayHello();
	}
	

}
